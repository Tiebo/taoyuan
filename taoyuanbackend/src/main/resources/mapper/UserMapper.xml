<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.taoyuanbackend.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.taoyuanbackend.pojo.User">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="username" column="user_username" jdbcType="VARCHAR"/>
            <result property="age" column="user_age" jdbcType="INTEGER"/>
            <result property="account" column="user_account" jdbcType="VARCHAR"/>
            <result property="phone" column="user_phone" jdbcType="CHAR"/>
            <result property="email" column="user_email" jdbcType="VARCHAR"/>
            <result property="photo" column="user_photo" jdbcType="VARCHAR"/>
            <result property="password" column="user_password" jdbcType="VARCHAR"/>
            <result property="interest" column="user_interest" jdbcType="VARCHAR"/>
            <result property="description" column="user_description" jdbcType="VARCHAR"/>
            <result property="gender" column="user_gender" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,user_username,user_age,
        user_account,user_phone,user_email,
        user_photo,user_password,user_interest,
        user_description,user_gender
    </sql>
</mapper>
